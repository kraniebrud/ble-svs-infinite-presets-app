{
  "name": "ble-svs-infinite-presets",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "dev": "bun build ./src/index.html --outdir=dist --sourcemap --target=browser --watch --define:process.env.NODE_ENV='\"development\"' --env='BUN_PUBLIC_*'",
    "build": "bun build ./src/index.html --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*'",
    "build:main": "bun build electron/main.ts --outfile=dist/main.js --target=node --external '*'",
    "build:preload": "bun build electron/preload.ts --outfile=dist/preload.js --target=node --external '*' --format=cjs",
    "electron:dev": "bun run build:main && bun run build:preload && concurrently -k \"bun run dev\" \"electron .\"",
    "electron:build": "bun run build && bun run build:main && bun run build:preload && electron-builder"
  },
  "dependencies": {
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.563.0",
    "react": "^19",
    "react-dom": "^19",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/web-bluetooth": "^0.0.21",
    "concurrently": "^9.2.1",
    "electron": "^40.1.0",
    "electron-builder": "^26.7.0",
    "wait-on": "^9.0.3"
  }
}